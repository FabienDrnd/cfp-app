.row
  .col-sm-12
    .page-header
      %h1 New Session Format
.row
  .col-md-6
    %fieldset
      = form_for @session_format, url: event_staff_session_formats_path(@event), html: {method: 'post', role: 'form'} do |f|
        = render partial: 'form', locals: {f: f}
        %button.btn.btn-success.pull-right{type: 'submit'} Save
