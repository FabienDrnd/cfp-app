$('#session-type-edit-<%= session_type.id %>').modal('hide');

var cells = $('table#session-types #session_type_<%= session_type.id %> td');
var sessionTypeName = '<%= session_type.name %>';

var data = [
  sessionTypeName,
  '<%=j truncate(session_type.description, length: 60) %>',
  '<%= session_type.duration %>',
  <% if session_type.public? %>
    "\u2713"
  <% else %>
    ''
  <% end %>
];

var length = data.length;
for (var i = 0; i < length; ++i) {
  cells[i].innerText = data[i];
}