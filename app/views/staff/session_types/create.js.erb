$('#session-type-new-dialog').modal('hide');

<% if session_type.persisted? %>
  $('#session-types').append('<%=j render session_type %>');
  clearFields([
    '#session_type_name',
    '#session_type_description',
    '#session_type_duration',
    '#session_type_public'
  ], '#session-type-new-dialog');
<% end %>

document.getElementById('flash').innerHTML = '<%=j show_flash %>';